MODULE main
VAR
  b_state : 1..10;
ASSIGN
  init(b_state) := 1;
  next(b_state) :=
    case
      b_state = 1  : 2;
      b_state = 2  : {2,3};
      b_state = 3  : {1,3,4};
      b_state = 4  : {1,4,5};
      b_state = 5  : {5,6};
      b_state = 6  : {6,7};
      b_state = 7  : {7,8};
      b_state = 8  : {8,9};
      b_state = 9  : 10;
      b_state = 10 : 10;
      TRUE         : b_state;
    esac;