MODULE main
VAR
  a_state : 1..11;

ASSIGN
  init(a_state) := 1;
  next(a_state) :=
    case
      a_state = 1   : 2;
      a_state = 2   : 3;
      a_state = 3   : 4;
      a_state = 4   : 5;
      a_state = 5   : { 6, 7, 8 };
      a_state = 6   : 5;
      a_state = 7   : 5;
      a_state = 8   : 9;
      a_state = 9   : { 10, 11 };
      a_state = 10  : 5;
      a_state = 11  : 1;
      TRUE            : a_state; 
    esac;
